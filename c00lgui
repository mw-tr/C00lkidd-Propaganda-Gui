local function HGSKAG_fake_script() -- CulGui.LocalScript 
	local script = Instance.new('LocalScript', CulGui)

	local supported = false
	local ReplicatedStorage = game:GetService('ReplicatedStorage')
	
	-- Loop through the descendants in ReplicatedStorage to check for the relevant RemoteEvents
	for _, v in pairs(ReplicatedStorage:GetDescendants()) do
		if v:IsA('RemoteEvent') and v.Name == 'UniversalEvent'then
			supported = true
			break  -- No need to continue the loop if we found one of the events
		end
	end
	
	-- Send the appropriate notification based on whether a supported event was found
	if supported then
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Game is Supported",
			Text = "Have Fun!",
			Duration = 5
		})
	else
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Game is not Supported",
			Text = "Try a different game :(",
			Duration = 5
		})
	end
	
end
coroutine.wrap(HGSKAG_fake_script)()
local function KUASL_fake_script() -- CloseOpen.LocalScript 
	local script = Instance.new('LocalScript', CloseOpen)

	-- Instances and Properties Setup (unchanged, as shown in your original code)
	-- (I have omitted the full setup for brevity. Make sure you include the same setup for your GUI elements)
	
	-- Close/Open Button functionality
	local CloseOpen = script.Parent
	
	-- Toggle the frame visibility when the Close/Open button is clicked
	CloseOpen.MouseButton1Click:Connect(function()
		-- Reference the frame that you want to open/close
		local frame = script.Parent:WaitForChild("CulGui")
	
		-- Toggle the visibility of the frame
		if frame.Visible then
			frame.Visible = false
			CloseOpen.Text = "Open"  -- Change the text to "Open" when closed
		else
			frame.Visible = true
			CloseOpen.Text = "Close"  -- Change the text to "Close" when opened
		end
	end)
	
	-- Example code for other buttons (as in your original script), unchanged
	-- Add functionality for SetSkybox, DecalSpam, SetFire, etc.
	
end
coroutine.wrap(KUASL_fake_script)()
