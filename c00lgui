local function CAYC_fake_script() -- SetSkybox.LocalScript 
	local script = Instance.new('LocalScript', SetSkybox)

	script.Parent.MouseButton1Click:Connect(function()
	local ReplicatedStorage = game:GetService('ReplicatedStorage')
		local remoteevent = ReplicatedStorage:WaitForChild('UniversalEvent')
	--	local txt = script.Parent.Parent.TextLabel
		
	if remoteevent then
			remoteevent:FireServer("culEvent")
		else
			print('Game not supported')
		end
		
	end)
end
