-- Scripts:

local function DYYPN_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	
	
	
	
	-- Variables
	local TweenService = game:GetService("TweenService") -- TweenService
	local textLabel = script.Parent -- Assuming this script is inside the TextLabel
	
	-- Initial position: off-screen to the right
	textLabel.Position = UDim2.new(-0.2, -151,0.182, 0)  -- This places the TextLabel off-screen to the right
	
	-- Define the final position
	local finalPosition = UDim2.new(0, 55,0, 50)  -- This is the final position on the screen
	
	-- Tween Info (Duration, EasingStyle, EasingDirection, Repeat Count, Reverses)
	local tweenInfo = TweenInfo.new(
		0.5,  -- Duration (2 seconds for the movement)
		Enum.EasingStyle.Sine,  -- Easing style (Linear for a smooth constant speed)
		Enum.EasingDirection.InOut,  -- Easing direction (smooth in and out)
		0,  -- No repeat
		false  -- No reversing
	)
	
	-- Create the Tween
	local tweenGoal = { Position = finalPosition }
	local tween = TweenService:Create(textLabel, tweenInfo, tweenGoal)
	
	-- Function to reverse the tween after a delay
	local function reverseTween()
		-- Wait 1 second before reversing the tween
		wait(1)
	
		-- Reverse the tween by creating a new tween from final position back to the start position
		local reverseTween = TweenService:Create(textLabel, tweenInfo, { Position = UDim2.new(-0.2, -151,0.182, 0) })
		reverseTween:Play()  -- Play the reverse tween
	end
	
	-- Play the initial tween
	tween:Play()
	
	-- When the initial tween completes, reverse the tween after 1 second
	tween.Completed:Connect(function()
		reverseTween()
	end)
	
end
coroutine.wrap(DYYPN_fake_script)()
local function VOCOY_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	
	
	
	
	
	
	-- Variables
	local TweenService = game:GetService("TweenService") -- TweenService
	local ImageLabel = script.Parent -- Assuming this script is inside the TextLabel
	
	-- Initial position: off-screen to the right
	ImageLabel.Position = UDim2.new(-0.16, -60,0.525, 0)  -- This places the TextLabel off-screen to the right
	
	-- Define the final position
	local finalPosition = UDim2.new(0.337, 0,0.552, 0)  -- This is the final position on the screen
	
	-- Tween Info (Duration, EasingStyle, EasingDirection, Repeat Count, Reverses)
	local tweenInfo = TweenInfo.new(
		1,  -- Duration (2 seconds for the movement)
		Enum.EasingStyle.Circular,  -- Easing style (Linear for a smooth constant speed)
		Enum.EasingDirection.InOut,  -- Easing direction (smooth in and out)
		0,  -- No repeat
		false  -- No reversing
	)
	
	-- Create the Tween
	local tweenGoal = { Position = finalPosition }
	local tween = TweenService:Create(ImageLabel, tweenInfo, tweenGoal)
	
	-- Function to reverse the tween after a delay
	local function reverseTween()
		-- Wait 1 second before reversing the tween
		wait(1)
	
		-- Reverse the tween by creating a new tween from final position back to the start position
		local reverseTween = TweenService:Create(ImageLabel, tweenInfo, { Position = UDim2.new(-0.16, -60,0.525, 0) })
		reverseTween:Play()  -- Play the reverse tween
	end
	
	-- Play the initial tween
	tween:Play()
	
	-- When the initial tween completes, reverse the tween after 1 second
	tween.Completed:Connect(function()
		reverseTween()
	end)
	
	
	
	
end
coroutine.wrap(VOCOY_fake_script)()
local function VPOZ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local TweenService = game:GetService("TweenService")
	local frameFade = script.Parent -- Assuming this script is a child of the frame you want to fade
	
	-- Set the initial properties (this step is optional)
	frameFade.BackgroundTransparency = 0 -- Fully opaque
	
	-- Create the TweenInfo for the fade effect (1.5 seconds duration)
	local tweenInfo = TweenInfo.new(
		1.5, -- Duration of the fade
		Enum.EasingStyle.Quad, -- Easing style (smooth transition)
		Enum.EasingDirection.Out, -- Easing direction (fade out)
		0, -- Repeat count (0 means it won't repeat)
		false, -- Reverses the tween (set to false for one way)
		0 -- Delay time (0 means no delay)
	)
	
	-- Define the properties we want to change (BackgroundTransparency to 1)
	local goal = { BackgroundTransparency = 1 }
	
	-- Create the tween
	local tween = TweenService:Create(frameFade, tweenInfo, goal)
	
	-- Play the tween after 1.5 seconds
	wait(1.9)
	tween:Play()
	wait(1)
	frameFade:Destroy()
end
coroutine.wrap(VPOZ_fake_script)()
